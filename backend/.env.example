# API Keys for Food Services
# Rename this file to .env and fill in your API keys

# USDA FoodData Central API Key - REQUIRED for optimal food search performance
# Register at: https://fdc.nal.usda.gov/api-key-signup.html
# Note: The demo key (DEMO_KEY) has significant rate limits - use a real key for production
USDA_API_KEY=your_usda_api_key_here

# Edamam API Credentials - Used as a backup search service
# Register at: https://developer.edamam.com/
EDAMAM_APP_ID=your_edamam_app_id_here
EDAMAM_APP_KEY=your_edamam_app_key_here

# FatSecret API Credentials - Used for food search service
# Register at: https://platform.fatsecret.com/api/
FATSECRET_CLIENT_ID=your_client_id_here
FATSECRET_CLIENT_SECRET=your_client_secret_here

# Service Configuration
USE_MOCK_FATSECRET=true     # If true, uses mock FatSecret data (per testing/development)
USE_EDAMAM_ONLY=false        # If true, only Edamam will be used for food search
USE_EDAMAM_AGGREGATED=false  # If true, results from both APIs will be combined
USE_ENHANCED_EDAMAM=true     # If true, enhanced Edamam features will be enabled

# Other environment variables for your application
DATABASE_URL=your_database_url_here
PORT=8000
